{
    "meta": {
        "extract_noun_chunks": true,
        "how_many_matches": "one"
    },
    "patterns": [
        {
            "rulename": "indefinite",
            "tokens": [
                { "POS": "DET", "LOWER": { "IN": ["a", "an"] } },
                { "OP": "*" },
                { "POS": { "IN": ["NOUN", "PRON", "PROPN"] } }
            ]
        },
        {
            "rulename": "definite",
            "tokens": [
                { "POS": "DET", "LOWER": "the" },
                { "OP": "*" },
                { "POS": { "IN": ["NOUN", "PRON", "PROPN"] } }
            ]
        },
        {
            "rulename": "other",
            "tokens": [
                { "POS": "DET", "LOWER": { "NOT_IN": ["a", "an", "the"] } },
                { "OP": "*" },
                { "POS": { "IN": ["NOUN", "PRON", "PROPN"] } }
            ]
        },
        {
            "rulename": "none",
            "tokens": [
                { "POS": { "NOT_IN": ["DET"] }, "OP": "?" },
                { "POS": { "IN": ["NOUN", "PRON", "PROPN"] } }
            ]
        }
    ]
}
