{
    "meta": {
        "extract_noun_chunks": true,
        "how_many_matches": "one"
    },
    "patterns": [
        {
            "rulename": "1st",
            "tokens": [
                {
                    "POS": { "IN": ["NOUN", "PRON", "PROPN", "NUM"] },
                    "OP": "+",
                    "LOWER": { "IN": ["i", "we"] }
                }
            ]
        },
        {
            "rulename": "2nd",
            "tokens": [
                {
                    "POS": { "IN": ["NOUN", "PRON", "PROPN", "NUM"] },
                    "OP": "+",
                    "LOWER": "you"
                }
            ]
        },
        {
            "rulename": "3rd",
            "tokens": [
                {
                    "POS": { "IN": ["NOUN", "PRON", "PROPN", "NUM"] },
                    "OP": "+",
                    "LOWER": { "NOT_IN": ["i", "we", "you"] }
                }
            ]
        }
    ]
}
